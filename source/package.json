        "sass": "sass --style=compressed --watch ./src/styles/styles.scss:./src/shared/assets/css/styles.css",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
