{
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "semi": true,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^@/(.*)$",
        "^../(.*)",
        "^./(.*)"
    ],
    "plugins": [
        "@trivago/prettier-plugin-sort-imports"
    ]
}